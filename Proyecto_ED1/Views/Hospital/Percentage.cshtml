@model Proyecto_ED1.Models.Percentage

@{
    ViewData["Title"] = "Estadísticas";
}

<h1>Porcenatje de vacunados</h1>

<div>
    <hr />
    <dl class="row">
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.NotVaccinated)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.NotVaccinated)
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.Vaccinated)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Vaccinated)
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.PositivePercentage)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.PositivePercentage)%
        </dd>
    </dl>
</div>
<div>
    <a asp-action="Index">Regresar al menú principal</a>
</div>
